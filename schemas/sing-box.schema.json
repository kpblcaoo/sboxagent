{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.subbox.dev/sing-box.schema.json",
  "version": "1.0.0",
  "title": "Sing-box Configuration Schema",
  "description": "Schema for sing-box client configuration validation",
  "type": "object",
  "properties": {
    "log": {
      "type": "object",
      "properties": {
        "level": {
          "type": "string",
          "enum": ["trace", "debug", "info", "warn", "error", "fatal", "panic"],
          "default": "info"
        },
        "timestamp": {
          "type": "boolean",
          "default": true
        },
        "output": {
          "type": "string",
          "enum": ["stdout", "stderr", "file"],
          "default": "stdout"
        }
      }
    },
    "dns": {
      "type": "object",
      "properties": {
        "servers": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "tag": {
                "type": "string"
              },
              "address": {
                "type": "string"
              },
              "address_resolver": {
                "type": "string"
              },
              "address_strategy": {
                "type": "string",
                "enum": ["prefer_ipv4", "prefer_ipv6", "ipv4_only", "ipv6_only"]
              },
              "strategy": {
                "type": "string",
                "enum": ["prefer_ipv4", "prefer_ipv6", "ipv4_only", "ipv6_only"]
              },
              "detour": {
                "type": "string"
              }
            },
            "required": ["address"]
          }
        },
        "rules": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "domain": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "domain_suffix": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "domain_keyword": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "domain_regex": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "geosite": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "source_geoip": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "source_ip_cidr": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "source_port": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              },
              "source_port_range": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "port": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              },
              "port_range": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "process_name": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "process_path": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "package_name": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "user": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "user_id": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              },
              "clash_mode": {
                "type": "string"
              },
              "wifi_ssid": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "wifi_bssid": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "rule_set": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "invert": {
                "type": "boolean"
              },
              "outbound": {
                "type": "string"
              },
              "server": {
                "type": "string"
              },
              "disable_cache": {
                "type": "boolean"
              },
              "rewrite_ttl": {
                "type": "integer"
              },
              "fakeip": {
                "type": "boolean"
              }
            }
          }
        },
        "final": {
          "type": "string"
        },
        "strategy": {
          "type": "string",
          "enum": ["prefer_ipv4", "prefer_ipv6", "ipv4_only", "ipv6_only"],
          "default": "prefer_ipv4"
        },
        "disable_cache": {
          "type": "boolean",
          "default": false
        },
        "independent_cache": {
          "type": "boolean",
          "default": false
        },
        "reverse_mapping": {
          "type": "boolean",
          "default": false
        },
        "fakeip": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": false
            },
            "inet4_range": {
              "type": "string",
              "default": "198.18.0.0/15"
            },
            "inet6_range": {
              "type": "string",
              "default": "fc00::/18"
            }
          }
        }
      }
    },
    "inbounds": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "tag": {
            "type": "string"
          },
          "listen": {
            "type": "string",
            "default": "127.0.0.1"
          },
          "listen_port": {
            "type": "integer"
          },
          "sniff": {
            "type": "boolean",
            "default": true
          },
          "sniff_override_destination": {
            "type": "boolean",
            "default": false
          },
          "domain_strategy": {
            "type": "string",
            "enum": ["prefer_ipv4", "prefer_ipv6", "ipv4_only", "ipv6_only"],
            "default": "prefer_ipv4"
          },
          "users": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "username": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                },
                "uuid": {
                  "type": "string"
                },
                "flow": {
                  "type": "string"
                }
              }
            }
          },
          "tls": {
            "type": "object",
            "properties": {
              "enabled": {
                "type": "boolean",
                "default": false
              },
              "server_name": {
                "type": "string"
              },
              "alpn": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "min_version": {
                "type": "string"
              },
              "max_version": {
                "type": "string"
              },
              "cipher_suites": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "certificate_path": {
                "type": "string"
              },
              "key_path": {
                "type": "string"
              },
              "certificate": {
                "type": "string"
              },
              "key": {
                "type": "string"
              }
            }
          },
          "transport": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": ["tcp", "ws", "http", "quic", "grpc"]
              },
              "path": {
                "type": "string"
              },
              "host": {
                "type": "string"
              },
              "method": {
                "type": "string"
              },
              "headers": {
                "type": "object"
              },
              "max_early_data": {
                "type": "integer"
              },
              "early_data_header_name": {
                "type": "string"
              }
            }
          }
        },
        "required": ["type"]
      }
    },
    "outbounds": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "tag": {
            "type": "string"
          },
          "server": {
            "type": "string"
          },
          "server_port": {
            "type": "integer"
          },
          "method": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "uuid": {
            "type": "string"
          },
          "flow": {
            "type": "string"
          },
          "security": {
            "type": "string",
            "enum": ["none", "tls", "reality"]
          },
          "tls": {
            "type": "object",
            "properties": {
              "enabled": {
                "type": "boolean",
                "default": false
              },
              "server_name": {
                "type": "string"
              },
              "alpn": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "min_version": {
                "type": "string"
              },
              "max_version": {
                "type": "string"
              },
              "cipher_suites": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "certificate_path": {
                "type": "string"
              },
              "key_path": {
                "type": "string"
              },
              "certificate": {
                "type": "string"
              },
              "key": {
                "type": "string"
              },
              "utls": {
                "type": "object",
                "properties": {
                  "enabled": {
                    "type": "boolean",
                    "default": false
                  },
                  "fingerprint": {
                    "type": "string"
                  }
                }
              },
              "reality": {
                "type": "object",
                "properties": {
                  "public_key": {
                    "type": "string"
                  },
                  "short_id": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "transport": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": ["tcp", "ws", "http", "quic", "grpc"]
              },
              "path": {
                "type": "string"
              },
              "host": {
                "type": "string"
              },
              "method": {
                "type": "string"
              },
              "headers": {
                "type": "object"
              },
              "max_early_data": {
                "type": "integer"
              },
              "early_data_header_name": {
                "type": "string"
              }
            }
          }
        },
        "required": ["type"]
      }
    },
    "route": {
      "type": "object",
      "properties": {
        "rules": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "domain": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "domain_suffix": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "domain_keyword": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "domain_regex": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "geosite": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "source_geoip": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "source_ip_cidr": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "source_port": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              },
              "source_port_range": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "port": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              },
              "port_range": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "process_name": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "process_path": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "package_name": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "user": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "user_id": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              },
              "clash_mode": {
                "type": "string"
              },
              "wifi_ssid": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "wifi_bssid": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "rule_set": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "invert": {
                "type": "boolean"
              },
              "outbound": {
                "type": "string"
              },
              "balancer": {
                "type": "string"
              }
            }
          }
        },
        "final": {
          "type": "string"
        },
        "auto_detect_interface": {
          "type": "boolean",
          "default": false
        },
        "default_mark": {
          "type": "integer"
        },
        "override_android_vpn": {
          "type": "boolean",
          "default": false
        }
      }
    }
  }
} 